{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "author": "y.kobori <yuudai.kobori@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run watch",
    "test": "npm run lint",
    "clean": "rimraf public",
    "dev": "npm run develop",
    "develop": "npm run clean && cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --inline --open --progress",
    "lint": "eslint src/js",
    "lint-fix": "eslint src/js --fix",
    "lint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.0",
    "browser-sync": "^2.26.12",
    "browserify": "^15.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.13.1",
    "node-sass-glob-importer": "^5.3.0",
    "node-sass-package-importer": "^5.3.0",
    "popper.js": "^1.14.6",
    "portfinder": "^1.0.20",
    "postcss-loader": "^3.0.0",
    "prettier-eslint": "^8.8.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "read-config": "^2.0.0",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.44.2",
    "webpack-build-notifier": "^0.1.30",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "reset-css": "^4.0.1"
  }
}
