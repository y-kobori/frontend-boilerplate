{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "author": "y.kobori <yuudai.kobori@gmail.com>",
  "repository": "https://github.com/y-kobori/frontend-boilerplate.git",
  "license": "MIT",
  "scripts": {
    "start": "npm run watch",
    "test": "npm run lint",
    "clean": "rimraf public",
    "dev": "npm run develop",
    "develop": "npm run clean && cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --inline --open --progress",
    "lint": "eslint src/js",
    "lint-fix": "eslint src/js --fix",
    "lint-rule-validate": "eslint --print-config resources/js/app.js | eslint-config-prettier-check"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^10.0.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "copy-webpack-plugin": "^6.1.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "css-mquery-packer": "^1.2.4",
    "cssnano": "^4.1.10",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.5.0",
    "import-glob-loader": "^1.1.0",
    "js-yaml": "^3.14.0",
    "load-config-file": "^1.0.7",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^7.0.0",
    "popper.js": "^1.16.1",
    "portfinder": "^1.0.28",
    "postcss": "^8.0.9",
    "postcss-loader": "^4.0.2",
    "prettier-eslint": "^11.0.0",
    "pug": "^3.0.1",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-build-notifier": "^2.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-fix-style-only-entries": "^0.5.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "reset-css": "^5.0.1"
  }
}
